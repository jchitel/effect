{
    "name": "@effect/printer-ansi",
    "version": "0.45.0",
    "type": "module",
    "description": "An easy to use, extensible pretty-printer for rendering documents for the terminal",
    "exports": {
        "./package.json": "./package.json",
        ".": "./src/index.ts",
        "./*": "./src/*.ts",
        "./internal/*": null
    },
    "scripts": {
        "build": "pnpm build-esm && pnpm build-annotate && pnpm build-cjs && build-utils pack-v3",
        "build-esm": "tsc -b tsconfig.build.json",
        "check": "tsc -b tsconfig.json"
    },
    "dependencies": {
        "@effect/printer": "workspace:^"
    },
    "devDependencies": {
        "effect": "workspace:^"
    },
    "peerDependencies": {
        "effect": "workspace:^"
    }
}
